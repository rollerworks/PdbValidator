{
    "name": "rollerworks/pdb-validator",
    "description": "PHP domain-parser Validators for the Symfony Validator component",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php-domain-parser",
        "pdb",
        "symfony",
        "validator"
    ],
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "rollerworks/pdb-symfony-bridge": "^1.0",
        "symfony/validator": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4.2",
        "rollerscapes/standards": "^1.0",
        "symfony/framework-bundle": "^6.3",
        "symfony/phpunit-bridge": "^7.0 || ^v6.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\PdbValidator\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\PdbValidator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
